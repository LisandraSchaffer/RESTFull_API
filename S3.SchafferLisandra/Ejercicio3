//Ejercicio 3: Invertir una Cadena 
// Escribe una funci√≥n que reciba una cadena de texto y devuelva la cadena invertida. Por ejemplo, si la entrada es "JavaScript", la salida debe ser "tpircSavaJ". 

function invertirCadena(cadena) {
  return cadena.split('').reverse().join('');
}

let textoOriginal = "HelloPeople";
let textoInvertido = invertirCadena(textoOriginal);

console.log("Texto original:", textoOriginal);
console.log("Texto invertido:", textoInvertido);

