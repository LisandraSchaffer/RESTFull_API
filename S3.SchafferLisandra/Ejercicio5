//Ejercicio 5: Suma de Números en un Rango 
//Escribe una función que reciba dos números como parámetros, “start” y “end”, y devuelva la suma de todos los números en ese rango (inclusive). 

function sumaEnRango(start, end) {
    let suma = 0;

    let inicio = Math.min(start, end);
    let fin = Math.max(start, end);
  
    for (let i = inicio; i <= fin; i++) {
      suma += i;
    }
  
    return suma;
  }
  
  let inicio = 1;
  let fin = 9;
  
  let resultado = sumaEnRango(inicio, fin);
  
  console.log(`La suma de los números entre ${inicio} y ${fin} es: ${resultado}`);